{
    "$schema": "./appsettings-schema.json",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        }
    },
    "ConnectionStrings": {
        "umbracoDbDSN": "",
        "umbracoDbDSN_ProviderName": ""
    },
    "Umbraco": {
        "CMS": {
            "Unattended": {
                "UpgradeUnattended": true
            },
            "Hosting": {
                "Debug": true
            },
            "Content": {
                "ContentVersionCleanupPolicy": {
                    "EnableCleanup": true
                }
            },
            "RequestHandler": {
                "AddTrailingSlash": false
            },
            "Global": {
                "Id": "2fa8c982-7c9f-41d4-abb4-35c1ace9ef55"
            },
            "ModelsBuilder": {
                "ModelsMode": "SourceCodeManual"
            },
            "WebRouting": {
                "DisableRedirectUrlTracking": true
            }
        }
    },
    "ForwardedForHeaderName": "X-Real-IP",
    "UrlTracker": {
        "HasDomainOnChildNode": true,
        "AppendPortNumber": true,
        "Enable": true,
        "Backoffice": {
            "Notifications": {
                "Enable": false
            }
        },
        "Pipeline": {
            "Enable": true,
            "EnableClientErrorTracking": true
        },
        "BlockedUrlsList": [
            "example.com",
            "example.nl"
        ],
        "AllowedUserAgents": []
    }
}